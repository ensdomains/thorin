{
    "extends":"../tsconfig.json",
    "compilerOptions": {
        "types": ["vite/client", "vite-plugin-svgr/client", "node"],
        "outDir": "dist",
        "declaration": true,
        "baseUrl": ".",
        "paths": {
            "!/*": ["../*"],
            "@/*": ["./*"],
            "test-utils": ["./test"]
        },
    },
    "exclude": ["test/**/*", "src/**/*.test.tsx", "src/**/*.test.ts"],
    "include": ["src/**/*"]
}